{
  "webroot": "wwwroot",
  "version": "2.0.0-*",
  "dependencies": {
    "Microsoft.AspNetCore.Server.Kestrel": "1.0.0-*",
    "Microsoft.AspNetCore.Server.IISIntegration": "1.0.0-*",
    "Microsoft.NETCore.App": {
      "version": "1.0.0-*",
      "type": "platform"
    },
    "Orchard.Hosting.Console": "2.0.0-*",
    "Orchard.Hosting.Web": "2.0.0-*",
    "System.Linq": "4.1.0-rc2-24027"
  },
  "runtimeOptions": {
    "gcServer": true,
    "gcConcurrent": true
  },
  "buildOptions": {
    "emitEntryPoint": true,
    "debugType": "portable",
    "preserveCompilationContext": true,
    "copyToOutput": {
      "include": [ "Tools" ],
      "mappings": {
        "csc.dll": "Tools/csc.dll"
      }
    },
    "compile": {
      "exclude": [
        "wwwroot",
        "node_modules",
        "App_Data",
        "Modules",
        "Themes"
      ]
    }
  },
  "frameworks": {
    "netcoreapp1.0": {
      "imports": [
        "dnxcore50",
        "portable-net451+win8"
      ]
    }
  },
  "tools": {
    "Microsoft.AspNetCore.Server.IISIntegration.Tools": {
      "version": "1.0.0-*",
      "imports": [
        "dnxcore50",
        "portable-net451+win8"
      ]
    }
  },
  "packExclude": [
    "node_modules",
    "bower_components",
    "**.kproj",
    "**.user",
    "**.vspscc"
  ],
  "packOptions": {
    "type": "git",
    "url": "https://github.com/OrchardCMS/Orchard2"
  },
  "scripts": {
    "prepublish": [ "gulp publish" ],
    "postrestore": [ "gulp publish" ]
  },
  "publishOptions": {
    "include": [
      "app_data",
      "modules",
      "themes",
      "views",
      "_ViewStart.cshtml",
      "web.config"
    ],
    "exclude": [
      "**/obj"
    ],
    "includeFiles": [ ],
    "excludeFiles": [ ],
    "builtIns": [ ],
    "mappings": {
      "csc.dll": "Tools/csc.dll"
    }
  },
  "commands": {
    "web": ""
  }
}