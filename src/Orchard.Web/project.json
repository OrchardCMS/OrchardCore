{
  "webroot": "wwwroot",
  "version": "2.0.0-*",
  "dependencies": {
    "Microsoft.AspNetCore.Server.Kestrel": "1.0.0-*",
    "Microsoft.NETCore.Platforms": "1.0.1-*",
    "Orchard.Hosting.Console": "2.0.0-*",
    "Orchard.Hosting.Web": "2.0.0-*"
  },
  "compilationOptions": {
    "emitEntryPoint": true,
    "define": [ "TRACE" ],
    "warningsAsErrors": true
  },
  "commands": {
    "web": "Orchard.Web"
  },
  "frameworks": {
    "netstandardapp1.5": {
      "dependencies": {
        "NETStandard.Library": "1.5.0-*"
      },
      "imports": [
        "dnxcore50",
        "portable-net451+win8"
      ]
    }
  },
  "exclude": [
    "wwwroot",
    "node_modules",
    "App_Data",
    "Core",
    "Modules",
    "Themes"
  ],
  "packExclude": [
    "node_modules",
    "bower_components",
    "**.kproj",
    "**.user",
    "**.vspscc"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/OrchardCMS/Orchard2"
  },
  "scripts": {
    "prepublish": [ "gulp publish" ],
    "postrestore": [ "gulp publish" ]
  }
}