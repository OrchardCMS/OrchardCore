{
  "version": "2.0.0-*",
  "dependencies": {
    "Microsoft.AspNetCore.Html.Abstractions": "1.0.0",
    "Microsoft.AspNetCore.Mvc": "1.0.0",
    "Microsoft.Extensions.Logging.Abstractions": "1.0.0",
    "Microsoft.AspNetCore.Mvc.Modules.Abstractions": "1.0.0-*",
    "Orchard.DependencyInjection": "2.0.0-*",
    "Orchard.Environment.Cache.Abstractions": "2.0.0-*",
    "Orchard.Environment.Extensions.Abstractions": "2.0.0-*",
    "Orchard.Environment.Shell.Abstractions": "2.0.0-*",
    "System.Reflection.Extensions": "4.0.1-*",
    "System.Diagnostics.TraceSource": "4.0.0-*",
    "Castle.Core": "4.0.0-alpha001"
  },
  "buildOptions": {
