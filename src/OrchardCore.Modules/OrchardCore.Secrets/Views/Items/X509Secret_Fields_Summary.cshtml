@model OrchardCore.Secrets.X509Secret

<p class="mb-0">
    <span class="badge text-bg-success">@T["X.509 Certificate"]</span>
    <span class="text-muted">@T["Store:"]: @Model.StoreLocation/@Model.StoreName</span>
    @if (!string.IsNullOrEmpty(Model.Thumbprint))
    {
        <br /><small class="text-muted font-monospace">@T["Thumbprint"]: @Model.Thumbprint</small>
    }
</p>
