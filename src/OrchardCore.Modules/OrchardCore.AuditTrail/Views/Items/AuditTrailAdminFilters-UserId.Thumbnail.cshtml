@model ShapeViewModel<AuditTrailIndexOptions>
@{
    var term = Model.Value.FilterResult.FirstOrDefault(x => x.TermName == "userid");
}

<h4 class="card-title">@T["User Id"]</h4>

<pre class="mb-3">@(@term != null ? "userid:" + @term.ToString() : "userid:...")</pre>
<p>@T["Filters by the id of a user."]</p>
<div class="d-block text-end">
    <span >
        <i class="fas fa-sm fa-bars text-primary" aria-hidden="true"></i>
    </span>
</div>
