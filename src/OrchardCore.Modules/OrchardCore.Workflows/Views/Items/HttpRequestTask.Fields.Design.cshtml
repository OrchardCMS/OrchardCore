@model ActivityViewModel<HttpRequestTask>
@using OrchardCore.Workflows.Http.Activities
<header>
    <h4><i class="fa-solid fa-globe" aria-hidden="true"></i>@Model.Activity.GetTitleOrDefault(() => T["HTTP Request"])</h4>
</header>
<span>@T["<em>{0} {1}</em>", Model.Activity.HttpMethod, Model.Activity.Url]</span>
