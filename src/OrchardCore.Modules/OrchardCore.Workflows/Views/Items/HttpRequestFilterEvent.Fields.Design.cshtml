@model ActivityViewModel<HttpRequestFilterEvent>
@using OrchardCore.Workflows.Http.Activities

<header>
    <h4><i class="fa-solid fa-bolt" aria-hidden="true"></i>@Model.Activity.GetTitleOrDefault(() => T["Filter Incoming HTTP Request"])</h4>
</header>
<span><em>@Model.Activity.HttpMethod</em></span>
<ul>
    @foreach (var routeValue in Model.Activity.RouteValues)
    {
        <li>@routeValue.Key = @routeValue.Value</li>
    }
</ul>
