{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "allowJs": true,
    "checkJs": false,
    "jsx": "react",
    "removeComments": true,
    "pretty": true,
    "skipLibCheck": true,
    "strict": false,
    "moduleResolution": "node",
    "esModuleInterop": true
  },
  "include": [
    "./Assets/js/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}
