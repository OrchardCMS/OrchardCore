{% assign link = Model.ContentItem.Content.LinkMenuItemPart %}

{% if Model.HasItems %}
<a class="nav-link dropdown-toggle" href="{{ link.Url | href }}" role="button" data-bs-toggle="dropdown" aria-expanded="false">
    {{ Model.ContentItem.DisplayText }}
</a>
{% else %}
<a class="nav-link" href="{{ link.Url | href }}">{{ Model.ContentItem.DisplayText }}</a>
{% endif %}
