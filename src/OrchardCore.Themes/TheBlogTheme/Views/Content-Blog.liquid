{% zone "Header" %}
<!-- Page Header -->
<!-- Set your background image for this header on the line below. -->
<header class="masthead" style="background-image: url('{{ "~/TheBlogTheme/img/home-bg.jpg" | href }}')">
    <div class="overlay"></div>
    <div class="container">
        <div class="row">
            <div class="col-lg-8 col-md-10 mx-auto">
                <div class="site-heading">
                    <h1>{{ Model.ContentItem.Content.TitlePart.Title }}</h1>
                    <span class="subheading">{{ Model.Content.BodyPart | shape_render }}</span>
                </div>
            </div>
        </div>
    </div>
</header>
{% endzone %}

{% shape_remove_item Model.Content "BodyPart" %}

<ul>
    <li>A</li>
    <li>
        <ul>
            <li>A1</li>
            <li>A2</li>
            <li>
                <ul>
                    <li>A2A</li>
                </ul>
            </li>
        </ul>
    </li>
</ul>
{% cache "a" %}
A {{ "now" | date: "%T" }} (No Duration) <br />
{% cache "a1", tags: "a1", contexts: "user", fixed_duration: "0:5:0" %}
A1 {{ "now" | date: "%T" }} (5 Minutes) <br />
{% endcache %}
{% cache "a2", tags: "a2", contexts: "user", fixed_duration: "0:0:1" %}
A2 {{ "now" | date: "%T" }} (1 Second) <br />
{% cache "a2a", tags: "a2a", contexts: "user", fixed_duration: "0:0:5" %}
A2A {{ "now" | date: "%T" }} (5 Seconds) <br />
{% endcache %}
{% endcache %}
{% endcache %}


{% cache "menublock" %}
    {% shape "menu", alias: "alias:main-menu", cache_id: "main-menu2", cache_duration: "00:05:00", cache_tag: "alias:main-menu" %}
{% endcache %}

{% cache "blog" %}
{{ Model.Content | shape_render }}
{% endcache %}

{% cache "nochildren" %}
This cache block should have no contexts or tags
{% endcache %}