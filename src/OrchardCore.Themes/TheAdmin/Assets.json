[
  {
    "generateRTL": false,
    "inputs": [
      "Assets/scss/index.scss"
    ],
    "watch": [
      "Assets/scss/components/**/*.scss",
      "Assets/scss/main/**/*.scss",
      "Assets/scss/mixins/**/*.scss",
      "Assets/scss/themes/**/*.scss"
    ],
    "output": "wwwroot/css/TheAdmin.css"
  },
  {
    "generateRTL": true,
    "inputs": [
      "Assets/scss/admin-layout.scss"
    ],
    "watch": [
      "Assets/scss/admin/**/*.scss"
    ],
    "output": "wwwroot/css/admin-layout.css"
  },
  {
    "inputs": [
      "Assets/js/*.js"
    ],
    "watch": [
      "Assets/js/*.js"
    ],
    "output": "wwwroot/js/TheAdmin.js"
  },
  {
    "generateRTL": false,
    "inputs": [
      "Assets/scss/login.scss"
    ],
    "watch": [
      "Assets/scss/componenets/_validations.scss"
    ],
    "output": "wwwroot/css/login.css"
  },
  {
    "inputs": [
      "Assets/js/header/global/*.js",
      "Assets/js/header/main/*.js"
    ],
    "watch": [
      "Assets/js/header/global/*.js",
      "Assets/js/header/main/*.js"
    ],
    "output": "wwwroot/js/TheAdmin-main.js"
  }
]
