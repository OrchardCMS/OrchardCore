{
  "webroot": "wwwroot",
  "version": "2.0.0-*",
  "dependencies": {
    "Microsoft.AspNetCore.Diagnostics": "1.1.0",
    "Microsoft.AspNetCore.Modules": "1.0.0-*",
    "Microsoft.AspNetCore.Server.IISIntegration": "1.1.0",
    "Microsoft.AspNetCore.Server.Kestrel": "1.1.0",
    "Microsoft.Extensions.Logging.Console": "1.1.0",
    "Orchard.Hosting.Console": "2.0.0-*",
    "Orchard.Environment.Shell.Data": "2.0.0-*",
    "NETStandard.Library": "1.6.1",
    "Orchard.Alias": "2.0.0-*",
    "Orchard.Autoroute": "2.0.0-*",
    "Orchard.Body": "2.0.0-*",
    "Orchard.ContentFields": "2.0.0-*",
    "Orchard.ContentPreview": "2.0.0-*",
    "Orchard.Contents": "2.0.0-*",
    "Orchard.ContentTypes": "1.0.0-*",
    "Orchard.Demo": "1.0.0-*",
    "Orchard.Deployment": "2.0.0-*",
    "Orchard.Deployment.Remote": "2.0.0-*",
    "Orchard.Admin": "2.0.0-*",
    "Orchard.Commons": "1.0.0-*",
    "Orchard.Diagnostics": "2.0.0-*",
    "Orchard.DynamicCache": "1.0.0-*",
    "Orchard.Modules": "2.0.0-*",
    "Orchard.Mvc": "2.0.0-*",
    "Orchard.Navigation": "2.0.0-*",
    "Orchard.Recipes": "2.0.0-*",
    "Orchard.Resources": "1.0.0-*",
    "Orchard.Roles": "2.0.0-*",
    "Orchard.Scripting": "2.0.0-*",
    "Orchard.Settings": "2.0.0-*",
    "Orchard.Setup": "2.0.0-*",
    "Orchard.Tenants": "2.0.0-*",
    "Orchard.Themes": "1.0.0-*",
    "Orchard.Users": "2.0.0-*",
    "Lucene": "2.0.0-*",
    "Orchard.Feeds": "2.0.0-*",
    "Orchard.Flows": "2.0.0-*",
    "Orchard.HomeRoute": "2.0.0-*",
    "Orchard.Indexing": "2.0.0-*",
    "Orchard.Layers": "2.0.0-*",
    "Orchard.Lists": "1.0.0-*",
    "Orchard.Markdown": "2.0.0-*",
    "Orchard.Menu": "2.0.0-*",
    "Orchard.Title": "2.0.0-*",
    "Orchard.Tokens": "2.0.0-*",
    "Orchard.Tokens.Content": "2.0.0-*",
    "Orchard.Widgets": "2.0.0-*",
    "Orchard.XmlRpc": "2.0.0-*",
    "SafeMode": "1.0.0-*",
    "TheAdmin": "1.0.0-*",
    "TheBlogTheme": "1.0.0-*",
    "TheTheme": "1.0.0-*",
    "Orchard.Nancy": "2.0.0-*"
  },
  "runtimeOptions": {
    "configProperties": {
      "System.GC.Server": true
    }
  },
	"buildOptions": {
		"emitEntryPoint": true,
		"preserveCompilationContext": true,
		"debugType": "portable",
		"compile": {
			"exclude": [
				"wwwroot",
				"node_modules",
				"App_Data",
				"Modules",
				"Themes"
			]
		}
	},
  "frameworks": {
    "netcoreapp1.1": {
      "dependencies": {
        "Microsoft.NETCore.App": {
          "version": "1.1.0-*",
          "type": "platform"
        }
      },
      "imports": [
        "dnxcore50",
        "portable-net451+win8"
      ]
    }
  },
  "tools": {
    "Microsoft.AspNetCore.Server.IISIntegration.Tools": "1.1.0-*",
    "Microsoft.AspNetCore.Mvc.Razor.ViewCompilation.Tools": "1.1.0-*"
  },
  "packExclude": [
    "node_modules",
    "bower_components",
    "**.kproj",
    "**.user",
    "**.vspscc"
  ],
  "packOptions": {
    "type": "git",
    "url": "https://github.com/OrchardCMS/Orchard2"
  },
  "scripts": {
    "prepublish": [ "gulp publish" ],
    "postrestore": [ "gulp publish" ]
  },
  "publishOptions": {
    "include": [
      "app_data",
      "modules",
      "themes",
      "views",
      "_ViewStart.cshtml",
      "wwwroot",
      "web.config"
    ],
    "exclude": [
      "**/obj"
    ],
    "includeFiles": [ "" ],
    "excludeFiles": [ "" ],
    "builtIns": [ "" ],
    "mappings": { }
  },
  "commands": {
    "web": ""
  }
}
